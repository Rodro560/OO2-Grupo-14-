<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="datos.Servicio" table="servicio">
		<id column="idServicio" name="idServicio">
			<generator class="identity"></generator>
		</id>
		<property name="nombre" column="nombre" type="string"/>
		<property name="descripcion" column="descripcion" type="string"/>
		<property name="durecionMin" column="durecionMin" type="int"/>
		<property name="precio" column="precio" type="float"/>
		<many-to-one name="prestador" class="datos.Prestador" column="idUsuario" not-null="true"/>
		<one-to-one name="especificacion" class="datos.Especificacion"/>
		<set name="lstDisponibilidad" cascade="save-update" table="disponibilidad" order-by="idDisponibilidad asc" inverse="true" lazy="false" fetch="select">
			<key column="idServicio" not-null="true"/>
			<one-to-many class="datos.Disponibilidad" />
		</set>
	</class>
</hibernate-mapping>